{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "placeholder-demo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/placeholderDemo/PlaceholderDemoWebPart.js",
          "manifest": "./src/webparts/placeholderDemo/PlaceholderDemoWebPart.manifest.json"
        }
      ]
    },
    "variant-theme-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/variantTheme/VariantThemeWebPart.js",
          "manifest": "./src/webparts/variantTheme/VariantThemeWebPart.manifest.json"
        }
      ]
    },
    "list-view-control-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/listViewControl/ListViewControlWebPart.js",
          "manifest": "./src/webparts/listViewControl/ListViewControlWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PlaceholderDemoWebPartStrings": "lib/webparts/placeholderDemo/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "VariantThemeWebPartStrings": "lib/webparts/variantTheme/loc/{locale}.js",
    "ListViewControlWebPartStrings": "lib/webparts/listViewControl/loc/{locale}.js"
  }
}
